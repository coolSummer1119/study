			<td>\${2+5}</td>
			<td>${2+5}</td>
		</tr>
		<tr>
			<td>\${"10"+5}</td>
			<td>${"10"+5}</td>
		</tr>
		<tr>
			<td>\${"10"+"5"}</td>
			<td>${"10"+"5"}</td>
		</tr>
		<tr>
			<td>\${"십"+"5"}</td>
			<td>에러 발생()</td>
		</tr>
		<tr>
			<td>\${4/5}</td>
			<td>${4/5}</td>
		</tr>
